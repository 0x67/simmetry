{
  "name": "api_server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/api_server/src",
  "targets": {
    "serve": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/api_server",
        "command": "cross-env CARGO_TARGET_DIR=../../dist/target/api_server cargo watch -c -w src -x \"run --color always\""
      }
    },
    "build": {
      "cache": true,
      "executor": "@monodon/rust:build",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/api_server"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "test": {
      "cache": true,
      "executor": "@monodon/rust:test",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/api_server"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "cache": true,
      "executor": "@monodon/rust:lint",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/api_server"
      }
    },
    "run": {
      "executor": "@monodon/rust:run",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/api_server"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    }
  },
  "tags": []
}
